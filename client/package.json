{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "jest --env=jsdom --setupFiles dotenv/config",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:ci": "jest --coverage --watchAll=false --ci",
    "test:lint": "eslint src --ext .ts,.tsx",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test:visual": "chromatic --exit-zero-on-changes",
    "eject": "react-scripts eject",
    "deploy": "node scripts/deploy.js",
    "deploy:supabase": "node scripts/deploy.js supabase",
    "deploy:frontend": "node scripts/deploy.js frontend",
    "deploy:test": "node scripts/deploy.js test",
    "deploy:full": "node scripts/deploy.js full",
    "setup": "npm install && npm run build",
    "dev": "concurrently \"npm run start\" \"cd supabase && supabase start\"",
    "db:push": "cd supabase && supabase db push",
    "db:studio": "cd supabase && supabase db studio",
    "functions:deploy": "cd supabase && supabase functions deploy --no-verify-jwt",
    "functions:logs": "cd supabase && supabase functions logs"
  },
  "dependencies": {
    "@craco/craco": "^7.1.0",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@supabase/supabase-js": "^2.76.0",
    "@tailwindcss/postcss": "^4.1.15",
    "@testing-library/dom": "^10.4.1",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.126",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/three": "^0.180.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "firebase": "^12.4.0",
    "lucide-react": "^0.546.0",
    "puppeteer": "^24.26.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.4",
    "react-scripts": "5.0.1",
    "recharts": "^3.3.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "three": "^0.180.0",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^9.1.13",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/react": "^9.1.13",
    "@storybook/react-vite": "^9.1.13",
    "@storybook/testing-library": "^0.2.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/jest-axe": "^3.5.5",
    "@types/recharts": "^1.8.29",
    "autoprefixer": "^10.4.21",
    "chromatic": "^13.3.1",
    "cypress": "^13.17.0",
    "dotenv": "^17.2.3",
    "jest": "^30.2.0",
    "jest-axe": "^9.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-websocket-mock": "^2.5.0",
    "msw": "^2.7.0",
    "postcss": "^8.5.6",
    "storybook": "^9.1.13",
    "tailwindcss": "^4.1.15",
    "ts-jest": "^29.2.6"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(msw|@mswjs|@testing-library)/)"
    ],
    "setupFiles": [
      "dotenv/config"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/src/__tests__/mocks/server.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.(ts|tsx|js)",
      "<rootDir>/src/**/?(*.)(spec|test).(ts|tsx|js)"
    ],
    "collectCoverageFrom": [
      "src/**/*.(ts|tsx)",
      "!src/**/*.d.ts",
      "!src/index.tsx",
      "!src/reportWebVitals.ts",
      "!src/setupTests.ts",
      "!src/**/*.stories.@(js|jsx|ts|tsx)",
      "!src/**/__tests__/**/*",
      "!src/**/test-utils.*"
    ],
    "coverageReporters": [
      "text",
      "lcov",
      "html",
      "json-summary"
    ],
    "coverageDirectory": "coverage",
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "testTimeout": 10000
  }
}
