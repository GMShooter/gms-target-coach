{
  "mcpServers": {
    "playwright": {
      "command": "npx",
      "args": [
        "@playwright/mcp@latest"
      ],
      "alwaysAllow": []
    },
    "firebase": {
      "command": "npx",
      "args": [
        "-y",
        "firebase-tools@14.15.2",
        "experimental:mcp"
      ]
    }
  }
}