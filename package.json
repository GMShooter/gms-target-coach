{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "jest --env=jsdom",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:ci": "jest --coverage --watchAll=false --ci",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write src/**/*.{ts,tsx}",
    "format:check": "prettier --check src/**/*.{ts,tsx}",
    "type-check": "tsc --noEmit",
    "pre-commit": "npm run lint && npm run type-check && npm run test:unit",
    "audit:fix": "npm audit fix",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test:visual": "chromatic --exit-zero-on-changes",
    "deploy": "node scripts/deploy.js",
    "deploy:supabase": "node scripts/deploy.js supabase",
    "deploy:frontend": "node scripts/deploy.js frontend",
    "deploy:test": "node scripts/deploy.js test",
    "deploy:full": "node scripts/deploy.js full",
    "setup": "npm install && npm run build",
    "dev": "concurrently \"npm run start\" \"cd supabase && supabase start\"",
    "db:push": "cd supabase && supabase db push",
    "db:studio": "cd supabase && supabase db studio",
    "functions:deploy": "cd supabase && supabase functions deploy --no-verify-jwt",
    "functions:logs": "cd supabase && supabase functions logs",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:dev": "docker-compose up app supabase",
    "docker:test": "docker-compose up test",
    "docker:e2e": "docker-compose up e2e",
    "docker:logs": "docker-compose logs -f",
    "docker:clean": "docker-compose down -v && docker system prune -f"
  },
  "dependencies": {
    "@craco/craco": "^7.1.0",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@sentry/integrations": "^7.114.0",
    "@sentry/react": "^10.22.0",
    "@sentry/tracing": "^7.120.4",
    "@supabase/supabase-js": "^2.76.0",
    "@tanstack/react-query": "^5.90.5",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@testing-library/dom": "^10.4.1",
    "@types/jwt-decode": "^2.2.1",
    "@types/node": "^20.19.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/three": "^0.180.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "firebase": "^12.4.0",
    "framer-motion": "^12.23.24",
    "jwt-decode": "^4.0.0",
    "logrocket": "^10.1.0",
    "lucide-react": "^0.546.0",
    "motion": "^12.23.24",
    "puppeteer": "^24.26.0",
    "qr-scanner": "^1.4.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.4",
    "react-scripts": "5.0.1",
    "recharts": "^3.3.0",
    "speckit": "^0.1.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "three": "^0.180.0",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4",
    "zustand": "^5.0.8"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^9.1.13",
    "@storybook/react": "^9.1.13",
    "@storybook/react-vite": "^9.1.13",
    "@storybook/testing-library": "^0.2.1",
    "@tailwindcss/postcss": "^4.1.16",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/jest-axe": "^3.5.5",
    "@types/recharts": "^1.8.29",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitejs/plugin-react-swc": "^4.1.0",
    "autoprefixer": "^10.4.21",
    "chromatic": "^13.3.1",
    "concurrently": "^9.2.1",
    "cypress": "^13.17.0",
    "dotenv": "^17.2.3",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-security": "^3.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.2.0",
    "jest-axe": "^9.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-websocket-mock": "^2.5.0",
    "msw": "^2.7.0",
    "postcss": "^8.5.6",
    "storybook": "^9.1.13",
    "tailwindcss": "^4.1.15",
    "ts-jest": "^29.2.6",
    "vite": "^5.4.0"
  }
}
